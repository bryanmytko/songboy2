{
  "name": "songboy2",
  "version": "1.0.0",
  "description": "Songboy 2.0",
  "main": "dist/index.js",
  "engines": {
    "node": "20.8.0"
  },
  "scripts": {
    "dokku": {
      "predeploy": "echo ${GOOGLE_CREDENTIALS} > /app/google-credentials.json",
      "postdeploy": "echo ${GOOGLE_CREDENTIALS} > /app/google-credentials.json"
    },
    "build": "tsc --build",
    "deploy-commands": "npm run build && node deploy-commands.js",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "jest"
  },
  "author": "Bryan Mytko",
  "license": "ISC",
  "dependencies": {
    "@bryanmytko/youtube.ts": "^0.2.10",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.16.0",
    "@distube/ytdl-core": "^4.13.7",
    "@google-cloud/text-to-speech": "^4.0.4",
    "discord.js": "^14.7.0",
    "dotenv": "^16.0.2",
    "ffmpeg": "^0.0.4",
    "html-entities": "^2.3.3",
    "i18n": "^0.15.1",
    "libsodium-wrappers": "^0.7.13",
    "mongoose": "^6.7.5",
    "openai": "^3.2.1",
    "pm2": "^5.2.2",
    "tslog": "^3.3.4"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.4",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.7.18",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
